{
  "name": "zaneevat/delivery-sdk-php",
  "description": "PHP library for REST API of popular transport companies",
  "keywords": [
    "PHP",
    "REST",
    "API"
  ],
  "type": "library",
  "homepage": "https://github.com/zaneevat/delivery-php-sdk",
  "license": "MIT",
  "authors": [
    {
      "name": "Maxim Semkin",
      "homepage": "https://github.com/zaneevat/"
    }
  ],
  "require": {
    "php": ">=8.1"
  },
  "autoload": {
    "psr-4": {
      "Delivery\\SDK\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Delivery\\SDK\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "phpstan/phpstan": "^1.9"
  }
}
